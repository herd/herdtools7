LISA s+o-assign+rl-deref-addr-rul (*7-light*)
{
x = 0;
y = 0;
}
 P0            | P1                 ;
               | f[lock]            ;
 w[once] y 2   | r[deref] r1 x      ;
               | mov r9 (xor r1 r1) ;
               | mov r2 (add y r9)  ;
 w[assign] x 1 | w[once] r2 1       ;
               | f[unlock]          ;
exists (y=2 /\ 1:r1=1)
