STR W1,[X2]  (* Memory (Write) Effect *) | DMB LD      ;
DMB ST                                   | LDR W3,[X4] ;
GCSSS1 X0    (* GCSSS1 Effect         *) |             ;

exists 1:X1=SSCap(x,5) /\ 1:X3=0  (* SSCap not supported in herd, yet *)
