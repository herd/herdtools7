AArch64 MP+bl-gcsb-dmb.sy+dmb.ld
variant=shadowstack
{

SS(x,1);

0:X2=y;
0:GCSPR_EL1=&x[1];

1:X2=y;
1:X4=x;

}

P0                                       | P1          ;
BL L0        (* GCS (Write) Effect    *) | LDR W1,[X2] ;
L0:                                      | DMB LD      ;
GCSB DSYNC   (* GCSB Effect           *) | LDR X3,[X4] ;
DMB SY                                   |             ;
