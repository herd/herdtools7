PPC B
"Overlaping arrows"
{
0:r2=x; 0:r4=y;
1:r2=y; 1:r4=x;
}
 P0           | P1           ;
 li r1,1      | lwz r1,0(r2) ;
 stw r1,0(r2) | cmpw r1,r1   ;
 li r3,1      | beq  LC00    ;
 stw r3,0(r4) | LC00:        ;
              | lwz r1,0(r2) ;
              | cmpw r1,r1   ;
              | beq  LC01    ;
              | LC01:        ;
              | lwz r3,0(r4) ;
exists (1:r1=1 /\ 1:r3=0)
