begin
  f1[] = f1[];
  f1 = f1;
  let a = f1;
  let b = f1[];
  let c = f2[4];
  f2[5] = 6;
  f3[12] = 13;
  f4[14] = 15;

  return 0;
end

forall(
     0: main.0.a = 3
  /\ 0: main.0.b = 3
  /\ 0: main.0.c = 7
  /\ 0: f3_storage = 12
  /\ 0: f4_storage = 15
