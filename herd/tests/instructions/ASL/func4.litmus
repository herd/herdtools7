ASL func4
(* Testing polymorphism on number of arguments. *)

{}

func f() => integer
begin
  return 0;
end

func f(x::integer) => integer
begin
  return x;
end

func f(x::integer, y::integer) => integer
begin
  return x + y;
end

func main()
begin
  let a = f();
  let b = f(1);
  let c = f(2, 3);
end

forall (
     0: main.0.a = 0
  /\ 0: main.0.b = 1
  /\ 0: main.0.c = 5
)
