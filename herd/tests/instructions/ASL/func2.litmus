ASL func02
(* Some basic getters/setters *)

{}

getter X_get[i::integer] => integer
begin
    return i;
end

setter X_set[i::integer] = v::integer
begin
    let internal_i = i;
    let internal_v = v;
end

func main()
begin
    X_set[2] = 3;
    let x = X_get[4];
end

forall (
     0: main.0.x = 4
  /\ 0: X_set.0.internal_i = 2
  /\ 0: X_set.0.internal_v = 3
)

