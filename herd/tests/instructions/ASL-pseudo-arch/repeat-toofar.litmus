ASL repeat-toofar

{
x = 0;
0:X1=x;
}

func T0(x:bits(64))
begin
  var s: integer = 0;
  var i = UInt(read_memory{32}(x));
  let j = i;
  repeat
    s = s + i;
    i = i-1;
  until i <= 0;
end;

func T1(x:bits(64))
begin
  for k = 1 to 3 do
    write_memory{32}(x,k[31:0]);
  end;
end;

func main() => integer
begin
  let x = read_register(1);

  T1(x);
  T0(x);

  return 0;
end;

