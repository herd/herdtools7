AArch64 A139
(* Tests whether we can load from a pointer - symbolic location+offset *)

{ int *pointer_to_x = x; int x[2]={4,42} }

P0;
  ADR X1, pointer_to_x; (* loads pointer_to_x into X1 *)
  LDR X1, [X1];         (* loads x into X1 *)
  LDR W0, [X1, #8];     (* loads x[1] into X1 *)

exists (0:X0 = 42)

